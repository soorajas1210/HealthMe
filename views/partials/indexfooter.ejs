<script>
  function addToCart(productId) {
    $.ajax({
      url: '/cart?id=' + productId,
      method: 'POST',
      success: function(response) {
        if (response.status) {
          Swal.fire({
            position: 'top-end',
            icon: 'success',
            title: 'Product is added to the cart',
            showConfirmButton: false,
            timer: 1500,
          });
          let count = $('#cart-count').html();
          count = parseInt(count) + 1;
          $('#cart-count').html(count);
        } else {
          Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: 'Please login',
            showConfirmButton: false,
            timer: 1500,
          });
          let count = $('#cart-count').html();
          count = parseInt(count) + 1;
          $('#cart-count').html(count);
        }
      },
    });
  }

  function addToWish(productId) {
    $.ajax({
      url: '/wishlist?id=' + productId,
      method: 'POST',
      success: function(response) {
        if (response.status) {
          Swal.fire({
            position: 'top-end',
            icon: 'success',
            title: 'Product is added to the wishlist',
            showConfirmButton: false,
            timer: 1500,
          });
          let count = $('#wish-count').html();
          count = parseInt(count) + 1;
          $('#wish-count').html(count);
        } else {
          Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: 'Please login',
            showConfirmButton: false,
            timer: 1500,
          });
          let count = $('#wish-count').html();
          count = parseInt(count) + 1;
          $('#wish-count').html(count);
        }
      },
    });
  }
</script>


